<table class='table'>
  <tr>
    <th>ID</th>
    <th>Username</th>
    <th>Email</th>
    <th>Created At</th>
    <th>Action</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.created_at %></td>
      <td><%= link_to 'show', admin_user_path(user) %></td>
    </tr>
  <% end %>
</table>
<%= paginate @users %>
